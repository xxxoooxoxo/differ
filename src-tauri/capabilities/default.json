{
  "$schema": "https://schema.tauri.app/config/2.0.0-rc/capability-schema",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "shell:allow-open",
    "dialog:allow-open",
    "dialog:default",
    "fs:allow-read-text-file",
    "fs:allow-exists",
    "opener:default"
  ]
}
